{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "linebreak-style": ["error", "windows"],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off"
    }
}
